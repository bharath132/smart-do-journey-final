{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://your-backend-url.com/:path*"
        }
    ],
    "redirects": [
        {
            "source": "/old-route",
            "destination": "/new-route",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                }
            ]
        }
    ]
}